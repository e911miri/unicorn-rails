<% @lectureschedules.each do |lectureschedule| %>
  <tr>
    <td><%= lectureschedule.course_id %></td>
    <td><%= lectureschedule.timefrom %></td>
    <td><%= lectureschedule.timeto %></td>
    <td><%= lectureschedule.day %></td>
    <td><%= lectureschedule.venue %></td>
    <td><%= lectureschedule.notes %></td>
    <td><%= link_to 'Show', lectureschedule %></td>
    <td><%= link_to 'Edit', edit_lectureschedule_path(lectureschedule) %></td>
    <td><%= link_to 'Destroy', lectureschedule, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

